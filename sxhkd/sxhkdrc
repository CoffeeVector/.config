# Binds

Print
    $HOME/bin/scripts/screenshot.sh

control + shift + Escape
    $TERM -e htop

super + backslash
    $TERM -e nvim

super + e
    $TERM -e ranger

super + equal
    $HOME/scripts/light/lightIncrease.sh

super + minus
    $HOME/scripts/light/lightDecrease.sh

super + shift + plus
    $HOME/scripts/volume/volumeIncrease.sh

super + shift + minus
    $HOME/scripts/volume/volumeDecrease.sh

super + shift + g
    $HOME/bin/scripts/otflatex.sh

super + m
    $HOME/scripts/dmenuCalc/dmenuCalc.sh

super + r
    $TERM -e rtv

super + s
    rofi -show ssh

super + slash
    $HOME/scripts/todo/todo.sh

super + shift + t
    $HOME/scripts/touchpad/touchpad.sh

super + Tab
    $HOME/scripts/computer/miscellaneous.sh

super + shift + Tab
    $HOME/scripts/computer/computer.sh

super + w
    rofi -show window
